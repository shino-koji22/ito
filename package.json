{
  "name": "ito-chat",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n server,web -c auto \"npm run dev --prefix server\" \"npm run dev --prefix web\"",
    "dev:server": "npm run dev --prefix server",
    "dev:web": "npm run dev --prefix web",
    "build": "npm run build --prefix web",
    "start": "node server/server.js"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
